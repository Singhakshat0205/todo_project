{ 


    
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
       
        "src": "/todos",       
        "dest": "index.js"
      },
      {
        "src":"/",
        "dest":"build/index.html"
      },
      {
        "src":"/add",
        "dest":"build/index.html"
      },
      {
       
        "src": "/todos/(.*)",       
        "dest": "index.js"
      },
      {
       
        "src": "/(.+)",       
        "dest": "build/$1"
      }
    ]
  
   
  }
  